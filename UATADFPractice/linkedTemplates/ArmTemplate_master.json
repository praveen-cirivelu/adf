{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "UATADFPractice"
		},
		"Src_Emp_Loop_Folder_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Src_Emp_Loop_Folder'"
		},
		"Tgrt_Archive_Connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Tgrt_Archive_Connection'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "UATADFPractice_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Src_Emp_Loop_Folder_connectionString": {
						"value": "[parameters('Src_Emp_Loop_Folder_connectionString')]"
					},
					"Tgrt_Archive_Connection_connectionString": {
						"value": "[parameters('Tgrt_Archive_Connection_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}